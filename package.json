  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-prod": "ng serve --configuration=production",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "deploy": "ng deploy --cname=webxr.art",
    "update": "ng update @angular/cli @angular/core",
    "update-next": "ng update @angular/cli@next @angular/core@next",
    "compress": "gltf-transform optimize src/assets/models/_VRGalleryOriginal1509comp2.glb src/assets/models/VRGalleryOriginal1509compressed1.glb --texture-compress webp",
    "test": "ng test",
    "lint": "npx @biomejs/biome format ./src --write",
    "docs": "typedoc --out docs projects/three"
  },
  "private": true,
